				<!-- Explain -->
				<table cellpadding="0" cellspacing="0" border="0" width="544">
					<tr>
						<td>
						<table cellpadding="0" cellspacing="0" border="0" align="left"> 
							<tr>
								<td><img src="images/jl09.jpg" border="0" width="174" height="125" onclick="javascript:pop('popup.asp?img_name=jl09',550,500,0)" style="cursor:hand"></td>
								<td width="10"><img src="/common/images/trans.gif" width="10" height="1" border="0"></td>
