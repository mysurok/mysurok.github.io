						<td height="25" style="padding-left:10px"><b><a href="cls_gg.asp" class="c">Gyeonggi-do</a><span class="c">,</span> Jeungpyon</b></td>
						<td align="right">
						<!-- select -->
						<select name="s02" onChange="window.open(this.options[this.selectedIndex].value,'_self')">
						<option value="" selected>Выбор блюда</option>
